{
  "name": "api",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "express": "^4.19.2",
    "@trpc/server": "^10.45.0",
    "@trpc/express": "^10.45.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.4",
    "tsx": "^4.16.2",
    "typescript": "^5.4.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": ["ts", "js", "json"],
    "testMatch": ["**/__tests__/**/*.test.ts"]
  }
}
